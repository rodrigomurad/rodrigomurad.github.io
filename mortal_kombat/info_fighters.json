info_fighters = '[ { "nombre": "Johnny Cage", "historia": "Johnny Cage es una superestrella de las Artes Marciales entrenada por grandes maestros de todo el mundo. Johnny Cage es el actual campeón de taquilla, y la estrella de películas como \'Dragon Fist\', \'Dragon Fist II\', así como también la premiada \'Violencia Súbita\'", "img": "johnny_cage.gif", "audio": "johnny_cage.mp3" }, { "nombre": "Kano", "historia": "Kano es un mercenario, matón, extorsionista y ladrón. Vive una vida de crimen e injusticia. Es un miembro devoto del Dragón Negro, un peligroso grupo de locos y asesinos, respetados dentro de los círculos internos del crimen", "img": "kano.gif", "audio": "kano.mp3" }, { "nombre": "Raiden", "historia": "Raiden es el nombre de una deidad conocida como el \'Dios del Trueno\'. Se rumorea que recibió una invitación personal del mismo Shang Tsung y tomó forma humana para competir en el Torneo", "img": "raiden.gif", "audio": "raiden.mp3" }, { "nombre": "Liu Kang", "historia": "Liu Kang pertenecía a la sociedad supersecreta del Loto Blanco. Dejó la sociedad para representar a los Templos Shaolin en el Torneo. Liu Kang es fuerte en sus creencias y desprecia a Shang Tsung", "img": "liu_kang.gif", "audio": "liu_kang.mp3" }, { "nombre": "Scorpion", "historia": "Scorpion al igual que Sub-Zero, se desconoce su verdadero nombre y origen. Ha mostrado de vez en cuando desconfianza y odio hacia Sub-Zero. Entre Ninjas, esto suele ser un signo de clanes opuestos", "img": "scorpion.gif", "audio": "scorpion.mp3" }, { "nombre": "Sub-Zero", "historia": "Sub-Zero se desconoce el nombre real o la identidad de este guerrero. Sin embargo, según las marcas de su uniforme, se cree que pertenece a Lin Kuei, un clan legendario de Ninjas Chinos", "img": "sub-zero.gif", "audio": "sub-zero.mp3" }, { "nombre": "Sonya Blade", "historia": "Sonya es miembro de una de las principales Unidades de las Fuerzas Especiales de Estados Unidos", "img": "sonya.gif", "audio": "sonya.mp3" } ]';